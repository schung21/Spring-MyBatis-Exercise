<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias alias="Emp"     type="oracle.java.omyBatis3.model.Emp" />
		<typeAlias alias="Dept"    type="oracle.java.omyBatis3.model.Dept" />
		<typeAlias alias="EmpDept" type="oracle.java.omyBatis3.model.EmpDept" />
		<typeAlias alias="Member1" type="oracle.java.omyBatis3.model.Member1" />
		<typeAlias alias="DeptVO"  type="oracle.java.omyBatis3.model.DeptVO" />
	</typeAliases>
	<mappers>
		<mapper resource="mybatis/Emp.xml" />
		<mapper resource="mybatis/Dept.xml" />
		<mapper resource="mybatis/EmpDept.xml" />
		<mapper resource="mybatis/Member1.xml" />
	</mappers>
</configuration>